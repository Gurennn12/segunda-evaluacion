#include<iostream>//Libreria de cabecera
using namespace std;

int main(){
	//definimos 3 variables
	int n1,n2,n3;
	cout<<"Introduce tres numeros enteros: ";
	cin>> n1>> n2>> n3;
	
	if(n1>n2&& n1>n3){
		cout<<"El mayor es ";
		cout<<"eres menor"<<n1;
	}
	return 0; 
}
